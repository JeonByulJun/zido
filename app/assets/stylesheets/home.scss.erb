#container	{width:800px;}
#header	{width:800px; height:100px;}
#content	{width:200px; height: auto;}
#sidebar	{overflow:auto;width:auto; height: 400px;float:left;}
#map {
float: left;
  width: 600px;
  height: 500px;
  background-color: #CCC;
}
#footer{
    clear:both;
    color: #616161;
    cursor: auto;
    font-family: verdana;
    font-size: 11px;
    font-style: normal;
    font-weight: bold;
    height: auto;
    letter-spacing: normal;
    text-align: left;
    text-decoration: underline;
    white-space: nowrap;
    width: auto;
}
